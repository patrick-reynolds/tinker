<% provide(:title, 'Bands') %>
<h1>Austin Bands</h1>

<ul class="bands">
  <% @bands.each do |band| %>
    <li>
      <div id="bname"><%= link_to band.name.titleize, band %></div>
      <div id="binfo"><%= link_to band.city, '#' %>
      <%= link_to band.state, '#' %></div>
    </li>
  <% end %>
</ul>